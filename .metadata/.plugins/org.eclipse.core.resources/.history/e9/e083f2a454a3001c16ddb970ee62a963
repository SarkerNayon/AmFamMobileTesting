package javaChalenges;

public class MaxInArray {

	public static void main(String[] args) {

		int a[] = { 11, 5, 20, 30, 10 ,2};
		int max = a[0];
		
//		for (int i = 0; i < a.length; i++) {
//			if (max < a[i]) {
//				max = a[i];
//			}
//		}
//		System.out.println(max);
		
		
		for (int i = 0; i < a.length; i++) {
			if (max > a[i]) {
				max = a[i];
			}
		}
		System.out.println(max);
	}
}
