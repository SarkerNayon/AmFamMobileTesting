package javaChalenges;

import java.util.ArrayList;
import java.util.List;

public class Q2_ArrayList {
	public static void main(String[] args) {
		final List<String> list = new ArrayList<>();
		list.add("Anik");//0
		list.add("Sakir");//1
		list.add("Munna");//2
		list.remove(2);
		System.out.println(list);
	}

}

//sakir :- 2
//nayeem :- 1, 3
// kanta :- 1,3
//mukta ;- 1
//akin :- wrong
